@using Dev_proc.Models.ViewModels.DeanViews
@model CreateDeanViewModel
@{
    ViewData["Title"] = "Home Page";
}

<form asp-action="CreateDeanPost" asp-controller="Dean" method="post" enctype="multipart/form-data">
    <p>
        <input name="Name" type="text" placeholder = "Dean name" class="form-control" />
    </p>
    <p>
        <input name="Email" type="text" placeholder = "Dean email" class="form-control" />
    </p>
    <p>
        <input name="Password" type="password" placeholder = "Dean password" class="form-control" />
    </p>
    <p>
        <input type="submit" value="Добавить" />
    </p>
</form>